package task2;

import java.util.*;

public class factorial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number:");
		int i = sc.nextInt();
		int ans = 1;
		
		for(int j=1;j<=i;j++) {
			ans=ans*j;
		}
		System.out.println("Factorial is: "+ ans );
		
	}

}
